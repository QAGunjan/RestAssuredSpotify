pipeline {
    agent any

    stages {
        stage('Geting Code from Github') {
            steps {
                git 'https://github.com/QAGunjan/RestAssuredSpotify.git'
            }
	}	
    stage('Build Clean') {
            steps {
                bat 'mvn clean'          
            }
    }
    stage('Build Code') {
            steps {
                bat 'mvn compile'
            }
    }
    stage('TestRunning') {
            steps {
                bat 'mvn clean test'         
            }
    }
    }
    }